{
    "id": "http://dojotoolkit.org/dijit/MenuItem",
    "name": "dijit.MenuItem",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dijit/MenuItem.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../dojo/dojo.js"
        }
    },
    "property": {
        "label": {
            "datatype": "string",
            "title": "Label"
        },
        "iconClass": {
            "datatype": "string",
            "title": "Icon Class"
        },
        "accelKey": {
            "datatype": "string",
            "title": "Shortcut Key"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        }
    },
    "content": "<span></span>"
}