{
    "id": "http://dojotoolkit.org/dijit/form/HorizontalRule",
    "name": "dijit.form.HorizontalRule",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dijit/form/HorizontalSlider",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dijit/form/HorizontalRule",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "count": {
            "datatype": "number",
            "defaultValue": 3,
            "title": "Count"
        },
        "ruleStyle": {
            "datatype": "string",
            "title": "Rule Style"
        },
        "container": {
            "datatype": "string",
            "description": "For HorizontalSlider, this is either \"topDecoration\" or \"bottomDecoration\",\nand indicates whether this rule goes above or below the slider.",
            "hidden": false
        }
    },
