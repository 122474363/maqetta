{
    "id": "http://dojotoolkit.org/dijit/Tooltip",
    "name": "dijit.Tooltip",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dijit/Tooltip.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../dojo/dojo.js"
        }
    },
    "property": {
        "label": {
            "datatype": "string",
            "title": "Label"
        },
        "_children": {
            "datatype": "string",
            "title": "Text"
        },
        "connectId": {
            "datatype": "array",
            "defaultValue": "[]",
            "title": "Connect IDs"
        },
        "showDelay": {
            "datatype": "number",
            "defaultValue": 400,
            "title": "Show Delay"
        },
        "position": {
            "datatype": "array",
            "defaultValue": "[]",
            "title": "Positions"
        },
        "defaultPosition": {
            "datatype": "array"
        }
    },
    "content": "<span></span>"
}