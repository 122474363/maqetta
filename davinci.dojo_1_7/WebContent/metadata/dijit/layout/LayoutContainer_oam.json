{
    "id": "http://dojotoolkit.org/dijit/layout/LayoutContainer",
    "name": "dijit.layout.LayoutContainer",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dijit/layout/LayoutContainer.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "isContainer": {
            "datatype": "boolean",
            "description": "Just a flag indicating that this widget descends from dijit._Container",
            "defaultValue": true,
            "hidden": true
        }
    },
    "childProperties": {
        "layoutAlign": {
            "datatype": "string",
            "option": [
                {
                    "value": "none"
                },
                {
                    "value": "left"
                },
                {
                    "value": "right"
                },
                {
                    "value": "top"
                },
                {
                    "value": "bottom"
                },
                {
                    "value": "client"
                }
            ],
            "defaultValue": "none",
            "title": "Layout Align"
        }
    },
    "content": "<span></span>"
}