{
    "id": "http://dojotoolkit.org/dojox/charting/plot2d/Columns",
    "name": "dojox.charting.plot2d/Columns",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/charting/plot2d/Columns.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../../dojo/dojo.js"
        }
    },
    "property": {
        "hAxis": {
            "datatype": "string"
        },
        "vAxis": {
            "datatype": "string"
        },
        "gap": {
            "datatype": "number",
            "title": "Gap"
        },
        "shadows": {
            "datatype": "array",
            "title": "Shadows",
            "defaultValue": "[0,0,0]"
        }
    },
    "content": "<span></span>"
}