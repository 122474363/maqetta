{
    "id": "http://dojotoolkit.org/dojox/data/XmlStore",
    "name": "dojox.data.XmlStore",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/data/XmlStore.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "jsId": {
            "datatype": "string",
            "title": "Variable Name"
        },
        "url": {
            "datatype": "string",
            "format": "url",
            "title": "URL"
        },
        "rootItem": {
            "datatype": "string",
            "title": "Root Item Tag Name"
        },
        "keyAttribute": {
            "datatype": "string",
            "title": "Key or Identifier Attribute"
        },
        "label": {
            "datatype": "string",
            "title": "Label Attribute"
        },
        "sendQuery": {
            "datatype": "boolean",
            "defaultValue": false,
            "title": "Send Additional Query String"
        }
    },
    "content": "<span></span>"
}