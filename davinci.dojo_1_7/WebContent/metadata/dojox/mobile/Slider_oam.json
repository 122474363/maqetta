{
    "id": "http://dojotoolkit.org/dojox/mobile/Slider",
    "name": "dojox.mobile.Slider",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "../dojox/mobile.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/parser.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/deviceTheme.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/compat.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/Slider.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "name": {
            "datatype": "string"
        },
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblSlider"
        },
        "value": {
            "datatype": "number",
            "defaultValue": 0
        },
        "min": {
            "datatype": "number",
            "defaultValue": 0
        },
        "max": {
            "datatype": "number",
            "defaultValue": 100
        },
        "step": {
            "datatype": "number",
            "defaultValue": 1
        },
        "flip": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "orientation": {
            "datatype": "string",
            "defaultValue": "auto"
        },
        "halo": {
            "datatype": "string",
            "defaultValue": "8pt"
        },
        "tabIndex": {
            "datatype": "number",
            "defaultValue": 0
        },
        "disabled": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<input type='range'>"
}