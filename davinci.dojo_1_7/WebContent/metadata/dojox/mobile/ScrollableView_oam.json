{
    "id": "http://dojotoolkit.org/dojox/mobile/ScrollableView",
    "name": "dojox.mobile.ScrollableView",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "../dojox/mobile.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/parser.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/deviceTheme.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/compat.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/ScrollableView.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "keepScrollPos": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "scrollBar": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "scrollDir": {
            "datatype": "string",
            "defaultValue": "v"
        },
        "fadeScrollBar": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "disableFlashScrollBar": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<div></div>"
}