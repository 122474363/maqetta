{
    "id": "http://dojotoolkit.org/dojox/mobile/Heading",
    "name": "dojox.mobile.Heading",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "back": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "slide"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "Heading"
        },
        "iconBase": {
            "datatype": "string"
        },
        "fixed": {
            "datatype": "string",
            "defaultValue": "",
            "option": [
                {
                    "value": ""
                },
                {
                    "value": "top"
                },
                {
                    "value": "bottom"
                }
            ]
        }
    },
    "content": "<h1 label='Heading'></h1>"
}