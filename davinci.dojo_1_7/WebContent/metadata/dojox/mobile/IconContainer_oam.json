{
    "id": "http://dojotoolkit.org/dojox/mobile/IconContainer",
    "name": "dojox.mobile.IconContainer",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "../dojox/mobile.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/parser.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/deviceTheme.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/compat.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/IconContainer.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "defaultIcon": {
            "datatype": "string"
        },
        "pressedIconOpacity": {
            "datatype": "number",
            "defaultValue": 0.4
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "below"
        },
        "iconBase": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "back": {
            "datatype": "string",
            "defaultValue": "Home"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "My Application"
        },
        "single": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<ul></ul>"
}