{
    "id": "http://dojotoolkit.org/dojox/mobile/ToolBarButton",
    "name": "dojox.mobile.ToolBarButton",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/parser",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide"
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "url": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "sync": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "label": {
            "datatype": "string",
            "defaultValue": "ToolBarButton"
        },
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<div label='ToolBarButton'></div>"
}