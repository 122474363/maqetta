{
    "id": "http://dojotoolkit.org/dojox/mobile/TabButton",
    "name": "dojox.mobile.TabButton",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "../dojox/mobile.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/parser.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/deviceTheme.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/compat.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/TabBarButton.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "none"
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon1": {
            "datatype": "string"
        },
        "icon2": {
            "datatype": "string"
        },
        "iconPos1": {
            "datatype": "string"
        },
        "iconPos2": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "url": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "sync": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "label": {
            "datatype": "string",
            "defaultValue": "TabBarButton"
        },
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<li label='TabBarButton'></li>"
}