{
    "id": "http://dojotoolkit.org/dojox/mobile/Carousel",
    "name": "dojox.mobile.Carousel",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.deviceTheme');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.compat');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.Carousel');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "numVisible": {
            "datatype": "number",
            "defaultValue": 3
        },
        "title": {
            "datatype": "string"
        },
        "pageIndicator": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "navButton": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "height": {
            "datatype": "string",
            "defaultValue": "300px"
        },
        "store": {
            "datatype": "object",
            "hidden": true
        },
        "query": {
            "datatype": "json",
            "hidden": true
        },
        "queryOptions": {
            "datatype": "json",
            "hidden": true
        }
    },
    "content": "<div></div>"
}