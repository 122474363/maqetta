{
    "id": "http://dojotoolkit.org/dojox/mobile/ContentPane",
    "name": "dojox.mobile.ContentPane",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.deviceTheme');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.compat');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.ContentPane');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "href": {
            "datatype": "string"
        },
        "content": {
            "datatype": "string"
        },
        "parseOnLoad": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "prog": {
            "datatype": "boolean",
            "defaultValue": true
        }
    },
    "content": "<div></div>"
}