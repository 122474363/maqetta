{
    "id": "http://dojotoolkit.org/dojox/mobile/CheckBox",
    "name": "dojox.mobile.CheckBox",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.deviceTheme');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.compat');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.CheckBox');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblCheckBox"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 1000
        },
        "checked": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "value": {
            "datatype": "string",
            "defaultValue": "on"
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<input type='checkbox'>"
}