{
    "id": "http://dojotoolkit.org/dojox/mobile/CheckBox",
    "name": "dojox.mobile.CheckBox",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "../dojox/mobile.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/parser.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/deviceTheme.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/compat.js",
            "module": "amd",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "src": "../dojox/mobile/CheckBox.js",
            "module": "amd",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblCheckBox"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 1000
        },
        "checked": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "value": {
            "datatype": "string",
            "defaultValue": "on"
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<input type='checkbox'>"
}