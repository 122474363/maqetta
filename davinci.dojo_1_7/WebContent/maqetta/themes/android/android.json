							"line-height",
							"overflow",
							"text-align",
							"-webkit-tap-highlight-color"
						],
						".mblSwitchOn.mblSwitchInner": [
							"left"
						],
						".mblSwitchBgLeft": [
							"background-image",
							"left",
							"width",
							"color",
							"background-color"
						],
						".mblSwitchBg": [
							"border-radius",
							"position",
							"top",
							"height",
							"border-width",
							"border-style",
