						],
						".mblSwitchText": [
							"position",
							"top",
							"width",
							"height",
							"padding",
							"line-height",
							"text-align"
						]
					},
					"query": ""
				},
				"Disabled": {
					"selectors": {
						".mblSwitchBgRight": [
							"background-image",
							"left",
							"width",
							"color",
							"background-color"
						],
						".mblSwitchTextRight": [
							"left"
						],
						".mblSwitchText": [
							"position",
							"top",
							"width",
							"height",
							"padding",
							"line-height",
							"text-align"
						]
					},
					"query": [
						".mblSwitchOn",
						".mblSwitchBgRight"
					],
					"simulate": [
						"mblSwitchOff",
						":style=''"
					]
				}
			},
			"subwidgets": {
