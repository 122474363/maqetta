{
    "dojox": {
        "Button": {
            "displayName": "Button",
            "states": {
                "Normal": {
                    "selectors": {
                        ".mblButton": [
                            "$std_10"
                        ]
                    },
                    "query": ""
                },
                "Disabled": {
                    "selectors": {
                        ".mblButtonDisabled": [
                            "$std_10"
                        ]
                    },
                    "query": ".mblButton",
                    "simulate": "mblButtonDisabled"
                },
                "Selected": {
                    "selectors": {
                        ".mblButtonSelected": [
                            "$std_10"
                        ]
                    },
                    "query": ".mblButton",
                    "simulate": "mblButtonSelected"
                }
            }
        },
        "EdgeToEdgeList": {
            "displayName": "EdgeToEdgeList",
            "states": {
                "Normal": {
                    "selectors": {
                        ".mblEdgeToEdgeList": [
                            "$std_10"
                        ]
                    },
                    "query": ""
                }
            },
            "subwidgets": {
                "ListItem": {
                    "displayName": "ListItem",
                    "states": {
                        "Normal": {
                            "selectors": {
                                ".mblListItem": [
                                    "position",
                                    "list-style-type",
                                    "vertical-align",
                                    "padding",
                                    "padding-top",
                                    "padding-right",
                                    "padding-bottom",
                                    "padding-left",
                                    "height",
                                    "border-bottom",
                                    "border-bottom-style",
                                    "border-bottom-width",
                                    "border-bottom-color",
                                    "background-color",
                                    "font-size",
                                    "color: white",
                                    "line-height"
                                ],
                                ".mblListItem .mblListItemAnchor": [
                                    "-webkit-tap-highlight-color",
                                    "background-position",
                                    "background-position-x",
                                    "background-position-y",
                                    "text-decoration",
                                    "padding-right"
                                ],
                                ".mblListItemTextBox": [
                                    "overflow",
                                    "white-space",
                                    "text-overflow"
                                ]
                            },
                            "query": ".mblListItem"
                        }
                    }
				},
				"MoveTo": {
					"displayName": "MoveTo",
					"states": {
						"Normal": {
							"selectors": {
								".mblListItemRightIcon": [
									"position",
									"float",
									"line-height",
									"margin-top",
									"margin-bottom"
								],
								".mblDomButtonGrayArrow": [
									"position",
									"width",
									"height"
								]
							},
							"query": ".mblListItemRightIcon"
						}
					}
				}
            }
		},
		"Heading": {
			"displayName": "Heading",
			"states": {
				"Normal": {
					"selectors": {
						".mblHeading": [
							"$std_10"
						]
					},
					"query": ""
				}
			}
		},
		"ToggleButton": {
			"displayName": "ToggleButton",
			"states": {
				"Normal": {
					"selectors": {
						".mblToggleButton": [
							"$std_10"
						]
					},
					"query": ""
				},
				"Disabled": {
					"selectors": {
						".mblToggleButton: disabled": [
							"$std_10"
						]
					},
					"query": ".mblToggleButton",
					"simulate": ": disabled='true'"
				},
				"Selected": {
					"selectors": {
						".mblToggleButton.mblToggleButtonChecked": [
							"$std_10"
						]
					},
					"query": ".mblToggleButton",
					"simulate": "mblToggleButtonChecked"
				}
			}
		},
		"View": {
			"displayName": "View",
			"states": {
				"Normal": {
					"selectors": {
						".mobilebody": [
							"$std_10"
						]
					},
					"query": ""
				}
			}
		},
		"Switch": {
			"displayName": "Switch",
			"states": {
				"Normal": {
					"selectors": {
						".mblSwitch": [
							"margin",
							"position",
							"width",
							"height",
							"line-height",
							"overflow",
							"text-align",
							"-webkit-tap-highlight-color"
						],
						".mblSwitchOn.mblSwitchInner": [
							"left"
						],
						".mblSwitchBgLeft": [
							"background-image",
							"left",
							"width",
							"color",
							"background-color"
						],
						".mblSwitchBg": [
							"border-radius",
							"position",
							"top",
							"height",
							"border-width",
							"border-style",
							"border-color",
							"font-family",
							"font-size",
							"font-weight",
							"line-height",
							"-webkit-box-sizing",
							"box-sizing"
						],
						".mblSwitchTextLeft": [
							"left"
						],
						".mblSwitchText": [
							"position",
							"top",
							"width",
							"height",
							"padding",
							"line-height",
							"text-align"
						]
					},
					"query": ""
				},
				"Disabled": {
					"selectors": {
						".mblSwitchBgRight": [
							"background-image",
							"left",
							"width",
							"color",
							"background-color"
						],
						".mblSwitchTextRight": [
							"left"
						],
						".mblSwitchText": [
							"position",
							"top",
							"width",
							"height",
							"padding",
							"line-height",
							"text-align"
						]
					},
					"query": [
						".mblSwitchOn",
						".mblSwitchBgRight"
					],
					"simulate": [
						"mblSwitchOff",
						": style=''"
					]
				}
			},
			"subwidgets": {
				"SwitchKnob": {
					"displayName": "SwitchKnob",
					"states": {
						"Normal": {
							"selectors": {
								".mblSwitchKnob": [
									"background-image",
									"background-color",
									"border-radius",
									"position",
									"top",
									"left",
									"width",
									"height",
									"font-size",
									"border-width",
									"border-style",
									"border-color",
									"-webkit-box-sizing",
									"box-sizing"
								]
							},
							"query": ".mblSwitchKnob"
						}
					}
				}
			}
		}
	}
}