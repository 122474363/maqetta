{
    "id": "http://dojotoolkit.org/dojox/mobile/IconContainer",
    "name": "dojox.mobile.IconContainer",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/IconContainer",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "defaultIcon": {
            "datatype": "string"
        },
        "pressedIconOpacity": {
            "datatype": "number",
            "defaultValue": 0.4,
            "option": [
                {"value": "slide"},
                {"value": "fade"},
                {"value": "flip"},
                {"value": "cover"},
                {"value": "coverv"},
                {"value": "dissolve"},
                {"value": "reveal"},
                {"value": "revealv"},
                {"value": "scaleIn"},
                {"value": "scaleOut"},
                {"value": "slidev"},
                {"value": "swirl"},
                {"value": "zoomIn"},
                {"value": "zoomOut"}
            ]
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "below"
        },
        "iconBase": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "back": {
            "datatype": "string",
            "defaultValue": "Home"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "My Application"
        },
        "single": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "editable": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<ul></ul>"
}