{
    "id": "http://www.ibm.com/oneui/form/CheckBoxList",
    "name": "oneui.form.CheckBoxList",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "themes/oneui/oneui.css",
            "$library": "oneui"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "oneui/form/CheckBoxList",
            "$library": "oneui"
        }
    ],
    "library": {
        "oneui": {
            "src": "../../../oneui"
        },
        "dojo": {
            "src": "../../../dojo.js"
        }
    },
    "property": {
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "tabIndex": {
            "datatype": "string",
            "defaultValue": "0",
            "title": "Tab Index"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        },
        "readOnly": {
            "datatype": "boolean",
            "title": "Read Only"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "test label:",
            "title": "Label"
        },
        "state": {
            "datatype": "string",
            "option": [
                {
                    "value": ""
                },
                {
                    "value": "Error"
                }
            ],
            "defaultValue": "",
            "title": "State"
        },
        "labelAlignment": {
            "datatype": "string",
            "option": [
                {
                    "value": "horizontal"
                },
                {
                    "value": "vertical"
                }
            ],
            "defaultValue": "horizontal",
            "title": "Label alignment"
        }
    },
    "content": "<select></select>"
}