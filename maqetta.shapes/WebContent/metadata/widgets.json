{
    "callbacks": "callbacks.js",
    "categories": {
        "shapes": {
            "name": "Drawing Tools",
            "widgetClass": "dijit"
        }
    },
    "widgets": [
	    {
	        "name": "Text",
	        "type": "shapes.Text",
	        "category": "shapes",
	        "icon": "resources/images/Overlay.gif",
	        "iconBase64": "data:image/gif;base64,",
	        "inlineEdit": {
	            "property": "content",
	            "displayOnCreate": true
	        },
	        "properties": {
	            "style": "font-size:14px"
	        },
	        "resizable": "none",
	        "helper": "davinci.libraries.shapes.shapes.TextHelper"
	    },
	    {
	        "name": "Horizontal Line",
	        "type": "shapes.Line",
	        "category": "shapes",
	        "icon": "resources/images/Overlay.gif",
	        "enablePointerEvents":true,
	        "iconBase64": "data:image/gif;base64,",
	        "resizable": "none",
	        "helper": "davinci.libraries.shapes.shapes.LineHelper"
	    },
	    {
	        "name": "Vertical Line",
	        "type": "shapes.Line",
	        "category": "shapes",
	        "icon": "resources/images/Overlay.gif",
	        "enablePointerEvents":true,
	        "iconBase64": "data:image/gif;base64,",
	        "resizable": "none",
	        "properties": {
	            "points": "0,100,0,0"
	        },
	        "helper": "davinci.libraries.shapes.shapes.LineHelper"
	    },
	    {
	        "name": "Line",
	        "type": "shapes.Line",
	        "category": "shapes",
	        "icon": "resources/images/Overlay.gif",
