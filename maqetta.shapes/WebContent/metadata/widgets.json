	        "properties": {
	            "points": "0,100,100,0"
	        },
	        "helper": "davinci.libraries.shapes.shapes.LineHelper"
	    },
	    {
	        "name": "Arrow",
	        "type": "shapes.Line",
	        "category": "shapes",
	        "icon": "resources/images/Overlay.gif",
	        "enablePointerEvents":true,
	        "iconBase64": "data:image/gif;base64,",
	        "resizable": "none",
	        "properties": {
	            "points": "0,100,100,0",
	            "endarrow": "forward"
	        },
	        "helper": "davinci.libraries.shapes.shapes.LineHelper"
	    },
	    {
	        "name": "Circle",
	        "type": "shapes.Circle",
	        "category": "shapes",
