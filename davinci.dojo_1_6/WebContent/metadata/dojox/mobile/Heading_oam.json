{
    "id": "http://dojotoolkit.org/dojox/mobile/Heading",
    "name": "dojox.mobile.Heading",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/mobile/themes/iphone/iphone.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "back": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "slide"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "Heading"
        }
    },
    "content": "<h1 label='Heading'></h1>"
