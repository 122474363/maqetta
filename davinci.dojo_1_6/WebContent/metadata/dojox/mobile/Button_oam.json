{
    "id": "http://dojotoolkit.org/dojox/mobile/Button",
    "name": "dojox.mobile.Button",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/mobile/themes/iphone/iphone.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "btnClass": {
            "datatype": "string",
            "defaultValue": "mblBlueButton"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 1000
        },
        "label": {
            "datatype": "string",
            "defaultValue": "Button"
        }
    },
    "content": "<button label='Button'></button>"
}