{
    "id": "http://dojotoolkit.org/dojox/mobile/ListItem",
    "name": "dojox.mobile.ListItem",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/mobile/themes/iphone/iphone.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "rightText": {
            "datatype": "string"
        },
        "btnClass": {
            "datatype": "string"
        },
        "anchorLabel": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "clickable": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "url": {
            "datatype": "string"
        },
        "transition": {
            "datatype": "string"
        },
        "sync": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "label": {
            "datatype": "string",
            "defaultValue": "ListItem"
        }
    },
    "content": "<li label='ListItem'></li>"
}