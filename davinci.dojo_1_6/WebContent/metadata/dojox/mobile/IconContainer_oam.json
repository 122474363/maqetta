{
    "id": "http://dojotoolkit.org/dojox/mobile/IconContainer",
    "name": "dojox.mobile.IconContainer",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/mobile/themes/iphone/iphone.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "defaultIcon": {
            "datatype": "string"
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "below"
        },
        "pressedIconOpacity": {
            "datatype": "number",
            "defaultValue": "0.4"
        },
        "iconBase": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "back": {
            "datatype": "string",
            "defaultValue": "Home"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "My Application"
        },
        "single": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<ul></ul>"
}