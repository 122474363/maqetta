{
    "id": "http://dojotoolkit.org/dojox/mobile/IconItem",
    "name": "dojox.mobile.IconItem",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/mobile/themes/iphone/iphone.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        }
    ],
    "library": {
        "dojo": {
            "version": "1.5",
            "src": "../../../"
        }
    },
    "property": {
        "lazy": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "requires": {
            "datatype": "string"
        },
        "timeout": {
            "datatype": "number",
            "defaultValue": "10"
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "clickable": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "url": {
            "datatype": "string"
        },
        "transition": {
            "datatype": "string"
        },
        "sync": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "label": {
            "datatype": "string",
            "defaultValue": "IconItem"
        }
    },
    "content": "<li label='IconItem'></li>"
}