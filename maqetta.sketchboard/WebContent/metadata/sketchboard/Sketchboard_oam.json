{
    "id": "http://www.maqetta.org/sketchboard/Sketchboard",
    "name": "Sketchboard",
    "spec": "1.0",
    "version": "1.0",
    "library": {
        "sketchboard": {
            "src": "../../../sketchboard"
        },
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "sketchboard/Sketchboard",
            "$library": "sketchboard"
        },
        {
            "type": "image",
            "src": "Sketchboard",
            "$library": "sketchboard"
        }
    ],
    "property": {
         "orientation": {
            "datatype": "string",
            "option": [
                {
                    "value": "portrait"
                },
                {
                    "value": "landscape"
                }
            ],
            "defaultValue": "portrait",
            "title": "Orientation"
        }
    },
    "content": "<div></div>"
}
